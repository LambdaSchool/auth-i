{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
      "node": true,
      "mocha": true
    },
    "rules": {
      "function-paren-newline": 0,
      "arrow-body-style": 0,
      "no-param-reassign": 0,
      "no-plusplus": 0,
      "linebreak-style": 0,
      "consistent-return": 0,
      "no-useless-return": 0,
      "no-return-assign": 0,
      "max-len": 0,
      "no-unused-vars": 0,
      "no-useless-constructor": 0,
      "no-extend-native": 0,
      "import/no-unresolved": 0,
      "no-restricted-properties": 0
    }
}
